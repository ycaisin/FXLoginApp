<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="195.0" prefWidth="291.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.UserController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="184.0" prefWidth="265.0" VBox.vgrow="ALWAYS">
      <children>
            <Label layoutX="37.0" prefHeight="35.0" prefWidth="236.0" text="Member Access only">
           	 	<font>
                  <Font size="24.0" />
            	</font>
            </Label>

            <TextField fx:id="inputUsername" layoutX="37.0" layoutY="41.0" prefHeight="35.0" prefWidth="215.0" promptText="username" />
            <PasswordField fx:id="inputPassword" layoutX="37.0" layoutY="89.0" prefHeight="35.0" prefWidth="215.0" promptText="password" />
            <Button layoutX="155.0" layoutY="154.0" mnemonicParsing="false" onAction="#signin" prefHeight="25.0" prefWidth="96.0" text="Enter" />
            <Label fx:id="inputError" layoutX="37.0" layoutY="130.0" prefHeight="17.0" prefWidth="215.0" textFill="#ee1212" />
            
      </children>
    </AnchorPane>
  </children>
</VBox>
